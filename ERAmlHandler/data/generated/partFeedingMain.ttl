feed:Transporting  a         owl:Class ;
        rdfs:comment         "See Figure 4 in Hansson, J‰rvenp‰‰. 2017. Modeling Capabilities for Functional Configuration of Part Feeding Equipment." ;
        rdfs:label           "Transporting" ;
        rdfs:subClassOf      feed:CombinedCapability ;
        spin:rule            [ a             sp:Construct ;
                               sp:templates  ( [ sp:object     [ sp:varName  "payload" ] ;
                                                 sp:predicate  feed:payload ;
                                                 sp:subject    spin:_this
                                               ]
                                             ) ;
                               sp:where      ( [ a              sp:Bind ;
                                                 sp:expression  [ a        feed:getPartCapability2 ;
                                                                  sp:arg1  spin:_this ;
                                                                  sp:arg2  feed:FingerGrasping
                                                                ] ;
                                                 sp:variable    [ sp:varName  "fingerGrasping" ]
                                               ]
                                               [ a              sp:Filter ;
                                                 sp:expression  [ a        sp:bound ;
                                                                  sp:arg1  [ sp:varName  "fingerGrasping" ]
                                                                ]
                                               ]
                                               [ sp:object     [ sp:varName  "fingerGrasping" ] ;
                                                 sp:predicate  feed:hasCapability ;
                                                 sp:subject    [ sp:varName  "graspingDevice" ]
                                               ]
                                               [ sp:object     [ sp:varName  "info" ] ;
                                                 sp:predicate  feed:hasBasicResourceInformation ;
                                                 sp:subject    [ sp:varName  "graspingDevice" ]
                                               ]
                                               [ sp:object     [ sp:varName  "mass" ] ;
                                                 sp:predicate  feed:mass ;
                                                 sp:subject    [ sp:varName  "info" ]
                                               ]
                                               [ sp:object     [ sp:varName  "gripperPayload" ] ;
                                                 sp:predicate  feed:payload ;
                                                 sp:subject    [ sp:varName  "fingerGrasping" ]
                                               ]
                                               [ a              sp:Bind ;
                                                 sp:expression  [ a        feed:getPartCapability2 ;
                                                                  sp:arg1  spin:_this ;
                                                                  sp:arg2  feed:Moving
                                                                ] ;
                                                 sp:variable    [ sp:varName  "moving" ]
                                               ]
                                               [ a              sp:Filter ;
                                                 sp:expression  [ a        sp:bound ;
                                                                  sp:arg1  [ sp:varName  "moving" ]
                                                                ]
                                               ]
                                               [ sp:object     [ sp:varName  "movingPayload" ] ;
                                                 sp:predicate  feed:payload ;
                                                 sp:subject    [ sp:varName  "moving" ]
                                               ]
                                               [ a              sp:Bind ;
                                                 sp:expression  [ a        sp:sub ;
                                                                  sp:arg1  [ sp:varName  "movingPayload" ] ;
                                                                  sp:arg2  [ sp:varName  "mass" ]
                                                                ] ;
                                                 sp:variable    [ sp:varName  "alternative" ]
                                               ]
                                               [ a              sp:Bind ;
                                                 sp:expression  [ a        sp:if ;
                                                                  sp:arg1  [ a        sp:gt ;
                                                                             sp:arg1  [ sp:varName  "alternative" ] ;
                                                                             sp:arg2  [ sp:varName  "gripperPayload" ]
                                                                           ] ;
                                                                  sp:arg2  [ sp:varName  "gripperPayload" ] ;
                                                                  sp:arg3  [ sp:varName  "alternative" ]
                                                                ] ;
                                                 sp:variable    [ sp:varName  "payload" ]
                                               ]
                                             )
                             ] ;
        owl:equivalentClass  [ a               owl:Restriction ;
                               owl:hasValue    feed:TransportingAssociation ;
                               owl:onProperty  feed:providesCapabilityAssociation
                             ] ;
        owl:equivalentClass  [ a               owl:Restriction ;
                               owl:hasValue    feed:MovingAssociation ;
                               owl:onProperty  feed:requiresCapabilityAssociation
                             ] ;
        owl:equivalentClass  [ a               owl:Restriction ;
                               owl:hasValue    feed:GraspingAssociation ;
                               owl:onProperty  feed:requiresCapabilityAssociation
                             ] .

feed:getPartCapability2
        a                spin:Function ;
        rdfs:label       "get part capability2" ;
        rdfs:subClassOf  spin:Functions ;
        spin:body        [ a                   sp:Select ;
                           sp:resultVariables  ( [ sp:varName  "capability" ]
                                               ) ;
                           sp:where            ( [ sp:object     spin:_arg1 ;
                                                   sp:predicate  feed:hasCapability ;
                                                   sp:subject    [ sp:varName  "combRecourse" ]
                                                 ]
                                                 [ sp:object     [ sp:varName  "device1" ] ;
                                                   sp:predicate  feed:hasComponent ;
                                                   sp:subject    [ sp:varName  "combRecourse" ]
                                                 ]
                                                 [ sp:object     [ sp:varName  "capability" ] ;
                                                   sp:predicate  feed:hasCapability ;
                                                   sp:subject    [ sp:varName  "device1" ]
                                                 ]
                                                 [ sp:object     spin:_arg2 ;
                                                   sp:predicate  rdf:type ;
                                                   sp:subject    [ sp:varName  "capability" ]
                                                 ]
                                               )
                         ] ;
        spin:constraint  [ a              spl:Argument ;
                           rdfs:comment   "The capability class of the child capability to be returned by this function" ;
                           spl:predicate  sp:arg2 ;
                           spl:valueType  owl:Class
                         ] ;
        spin:constraint  [ a              spl:Argument ;
                           rdfs:comment   "An instance of a Combined Capability for which the child capabilities are looked for" ;
                           spl:predicate  sp:arg1 ;
                           spl:valueType  feed:CombinedCapability
                         ] .

