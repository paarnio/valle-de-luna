:ConstructTemplatesVPA1
  rdf:type spin:ConstructTemplate ;
  spin:body [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object :Bicycle ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "bic"^^xsd:string ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "datetime"^^xsd:string ;
