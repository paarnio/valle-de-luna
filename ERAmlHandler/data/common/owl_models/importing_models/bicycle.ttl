accessories:Bell
  spin:constructor [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "datetime"^^xsd:string ;
              ] ;
            sp:predicate :created ;
            sp:subject spin:_this ;
          ]
        ) ;
      sp:where (
          [
            rdf:type sp:Optional ;
            sp:elements (
                [
                  sp:object [
                      sp:varName "exdate"^^xsd:string ;
                    ] ;
                  sp:predicate :created ;
                  sp:subject spin:_this ;
                ]
              ) ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:not ;
                sp:arg1 [
                    rdf:type sp:bound ;
                    sp:arg1 [
                        sp:varName "exdate"^^xsd:string ;
                      ] ;
                  ] ;
              ] ;
          ]
          [
            rdf:type sp:Bind ;
            sp:expression [
                rdf:type afn:now ;
              ] ;
            sp:variable [
                sp:varName "datetime"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
